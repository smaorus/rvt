@MISC{prophecyvars,
  author = {B. Jonsson and others},
  title = {In Search of the Future: Do we Need Prophecy Variables?},
  month = {June},
  year = {1993},
  note = {Draft Manuscript},
  abstract = {In the stepwise development of a distributed system, one has to verify
    that a specification at a lower level of abstraction correctly implements
    a specification at a higher level of abstraction. We consider specifications
    that are given as labelled transition systems (automata) with fairness
    requirements. Abadi and Lamport have suggested prophecy variables
    and backward simulations as a proof technique to be used in the case
    that the abstract specification makes nondeterministic choices ``in
    advance of'' the concrete specification. In this paper, we look more
    closely at this technique. We suggest alternatives to the backward
    simulation technique, and compare their relative power. One contribution
    is to suggest a new simulation technique, called delayed simulation,
    which in a large number of cases can replace both forward and backward
    simulation as a proof technique. We conclude the paper with a trick
    for circumventing the restriction to finitariness for prophecy variables.},
  file = {prophecy.ps.Z}
}

